{
  "name": "@milosmiric/semantic-cache",
  "version": "1.0.0",
  "description": "Reduce LLM API costs and latency with semantic caching. Uses pluggable vector stores and embedding providers - includes MongoDB Atlas and VoyageAI implementations",
  "type": "module",
  "main": "./dist/lib/index.mjs",
  "module": "./dist/lib/index.mjs",
  "types": "./dist/lib/index.d.mts",
  "exports": {
    ".": {
      "import": "./dist/lib/index.mjs",
      "types": "./dist/lib/index.d.mts"
    }
  },
  "bin": {
    "semantic-cache": "./dist/cli/index.mjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "bun run src/cli/index.ts",
    "cli": "bun run src/cli/index.ts",
    "demo": "bun run src/cli/index.ts demo",
    "query": "bun run src/cli/index.ts query",
    "stats": "bun run src/cli/index.ts stats",
    "clear": "bun run src/cli/index.ts clear",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "typecheck": "tsc --noEmit",
    "build": "tsdown",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "semantic-cache",
    "llm",
    "mongodb",
    "vector-search",
    "embeddings",
    "voyageai",
    "openai",
    "anthropic",
    "vercel-ai-sdk",
    "ai",
    "cache",
    "atlas"
  ],
  "author": "Miloš Mirić",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/milosmiric/semantic-cache"
  },
  "bugs": {
    "url": "https://github.com/milosmiric/semantic-cache/issues"
  },
  "homepage": "https://github.com/milosmiric/semantic-cache#readme",
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/node": "^22.15.32",
    "tsdown": "^0.17.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.79",
    "ai": "^5.0.108",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "mongodb": "^7.0.0",
    "voyageai": "^0.0.8",
    "zod": "^4.1.13"
  },
  "engines": {
    "node": ">=18"
  }
}
